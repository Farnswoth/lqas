<?xml version="1.0" encoding="UTF-8"?>
<suite name="TestingOpower on Android Emulator">
	
		<parameter name="browserName" value="android"/>
		<parameter name="ip" value="127.0.0.1"/>
		<!-- tests -->

	<test name="tests on Anroid Emulator" preserve-order="false">
	    <groups>
	        <run>
	            <include name="groupAndroid" />
	        </run>
	    </groups>
		<packages>
		    <package name="com.opower.testcase.testcategories.*"/>
		    <package name="com.opower.testcase.quit"/>
		</packages>
	</test>
	  
</suite>